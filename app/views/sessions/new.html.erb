<%= form_for(:session, url: login_path) do |f| %>
  <h1>Login</h1>
  <div class="field">
    <%= f.label :email %><br>
    <%= f.email_field :email , :required=> 'r'%>
  </div>
  <div class="field">
    <%= f.label :password %><br>
    <%= f.password_field :password , :required=> 'r'%>
  </div>
  <div class="actions">
    <%= f.submit "Login"%>
  </div>
<% end %>
